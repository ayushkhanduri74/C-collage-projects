int var1,var2,temp;
printf("Enter The Two Numbers: ");
scanf("%d%d",&var1,var2);

printf("Before using swaping\nFirst Variable = %d\nSecond Variable = %d\n");

var1 = var1 + var2;
var2 = var1 - var2;
var1 =  var1 - var2;

printf("After swaping\nFirst Variable = %d\nSecond Variable = %d\n",var1,var2); 


