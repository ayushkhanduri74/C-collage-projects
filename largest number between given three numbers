
//find the largest numbers in input 3 num

#include<stdio.h>
int main()
{
	int a,b,c;
	printf("Enter Three Numbers: ");
	scanf("%d%d%d",&a,&b,&c);
	
	if (a>=b && a>=c)
	   printf("\n%d Is The largest Number");
	if (b>=a && b>=c)
	   printf("\n%d Is The largest Number");
	if (c>=a && c>=b) 
	   printf("\n %d Is The largest Number");   
	
	
	
return 0;	
}
